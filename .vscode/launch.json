{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debugger (run_class_finetuning)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/videomamba/video_sm/run_class_finetuning.py",
      "console": "integratedTerminal",
      "args": [
        "--model=videomamba_middle",
        "--data_path=${workspaceFolder}/videomamba/video_sm/data_list/k400",
        "--prefix=${workspaceFolder}/rsu/data/k400",
        "--data_set=Kinetics_sparse",
        "--split=,",
        "--nb_classes=400",
        "--log_dir=${workspaceFolder}/videomamba/video_sm/exp/k400/videomamba_tiny/out/artifacats_f8x224",  
        "--output_dir=${workspaceFolder}/videomamba/video_sm/exp/k400/videomamba_tiny/out/artifacats_f8x224",
        "--batch_size=64",
        "--num_sample=2",
        "--input_size=224",
        "--short_side_size=224",
        "--save_ckpt_freq=100",
        "--num_frames=8",
        "--num_workers=12",
        "--warmup_epochs=5",
        "--tubelet_size=1",
        "--epochs=70",
        "--lr=2e-4",
        "--drop_path=0.1",
        "--aa=rand-m5-n2-mstd0.25-inc1",
        "--opt=adamw",
        "--opt_betas", "0.9", "0.999",
        "--weight_decay=0.1",
        "--test_num_segment=4",
        "--test_num_crop=3",
        "--dist_eval",
        // "--test_best",
        "--bf16"
      ]
      // "args": "${command:pickArgs}"
    }
  ]
}